{
  "name": "mern_app",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "14.17.0",
    "npm": "6.14.13"
  },
  "main": "src/server.js",
  "scripts": {
    "start": "npm run build && node ./build/index.js",
    "build": "npm run clean && npm run babel-build",
    "babel-build": "babel -d ./build ./src -s",
    "clean": "rm -rf ./build && mkdir build",
    "start-dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src --fix",
    "node-start": "npm run compile-run",
    "compile-run": "babel src --out-dir build --source-maps && node ./build/index.js",
    "heroku-prebuild": "rm -rf node_modules",
    "heroku-postbuild": "npm install && node ./build/index.js"

  },
  "keywords": [],
  "author": "",
  "license": "ISC",
