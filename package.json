    "aws-sdk": "^2.906.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.14",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "prettier": "^2.2.1",
    "socket.io": "^4.0.1",
    "@babel/cli": "^7.13.16",
    "@babel/preset-env": "^7.14.2",
    "@babel/core": "^7.14.2"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "nodemon": "^2.0.7"
  }
}
